.container {
  background-color: #fff;
}
.container .iheaderbox {
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding-bottom: 70rpx;
  margin-top: 50rpx;
}
.container .iheaderbox .headerTop {
  height: 114rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}
.container .iheaderbox .headerTop .headerTopLeft {
  height: 100%;
  display: flex;
  align-items: center;
}
.container .iheaderbox .headerTop .headerTopLeft .userImgUrl open-data {
  width: 100rpx;
  height: 100rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
}
.container .iheaderbox .headerTop .headerTopLeft .headerTopLeft_tit {
  height: 100%;
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
}
.container .iheaderbox .headerTop .headerTopLeft .headerTopLeft_tit .headerTopLeft_tit_t {
  font-size: 48rpx;
  font-weight: 500;
  margin-bottom: 10rpx;
}
.container .iheaderbox .headerTop .headerTopLeft .headerTopLeft_tit .headerTopLeft_tit_b {
  font-size: 26rpx;
  color: #999999;
}
.container .iheaderbox .headerTop .headerTopRight {
  display: flex;
  align-items: center;
}
.container .iheaderbox .headerTop .headerTopRight image:nth-child(1) {
  width: 50rpx;
  height: 50rpx;
}
.container .iheaderbox .headerTop .headerTopRight image:nth-child(2) {
  margin-left: 20rpx;
  width: 12rpx;
  height: 21rpx;
}
.container .iheaderbox .headerBottom {
  width: 690rpx;
  height: 100rpx;
  box-shadow: 0px 10px 10px rgba(229, 229, 229, 0.25);
  border-radius: 50rpx;
  margin-top: 95rpx;
  text-align: center;
  line-height: 100rpx;
  position: relative;
}
.container .iheaderbox .headerBottom image {
  width: 690rpx;
  height: 100rpx;
  border-radius: 50rpx;
}
.container .iheaderbox .headerBottom .headerBottom_tit {
  font-size: 30rpx;
  font-weight: 700;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%);
  color: #265b99;
}
.container .lighting {
  padding-top: 30rpx;
  margin-top: 20rpx;
  background: #fff;
  width: 690rpx;
  margin: 0 auto;
}
.container .lighting .lightingTop {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}
.container .lighting .lightingTop .lightingTop_l {
  display: flex;
  align-items: baseline;
}
.container .lighting .lightingTop .lightingTop_l .lightingTop_tit {
  width: 216rpx;
  height: 50rpx;
  font-size: 36rpx;
  margin-right: 20rpx;
}
.container .lighting .lightingTop .lightingTop_l .lightingTop_close_box {
  display: flex;
  align-items: baseline;
}
.container .lighting .lightingTop .lightingTop_l .lightingTop_close_box .lightingTop_close {
  font-size: 24rpx;
  color: #f5a623;
  margin-right: 10rpx;
}
.container .lighting .lightingTop .lightingTop_l .lightingTop_close_box .lightingTop_close_icon {
  width: 21rpx;
  height: 12rpx;
}
.container .lighting .lightingTop image {
  width: 30rpx;
  height: 30rpx;
}
.container .lighting .lightingCenter {
  display: flex;
  justify-content: space-between;
}
.container .lighting .lightingCenter .lightingCenterItem {
  width: 225rpx;
  display: flex;
  margin-right: 8rpx;
  flex-direction: column;
  justify-content: space-between;
  padding-bottom: 20rpx;
  border-bottom: 8rpx solid #e5e5e5;
}
.container .lighting .lightingCenter .lightingCenterItem:nth-child(3) {
  margin-right: 0rpx;
}
.container .lighting .lightingCenter .lightingCenterItem .lightingCenterItemIcon {
  height: 32rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.container .lighting .lightingCenter .lightingCenterItem .lightingCenterItemIcon .lightingCenterItemIcon_img1 {
  width: 34rpx;
  height: 32rpx;
}
.container .lighting .lightingCenter .lightingCenterItem .lightingCenterItemIcon .lightingCenterItemIcon_img2 {
  width: 18rpx;
  height: 34rpx;
}
.container .lighting .lightingCenter .lightingCenterItem .lightingCenterItemIcon .lightingCenterItemIcon_img3 {
  width: 32rpx;
  height: 34rpx;
}
.container .lighting .lightingCenter .lightingCenterItem .lightingCenterItemIcon text {
  margin-left: 10rpx;
  font-size: 20rpx;
  color: #999999;
}
.container .lighting .lightingCenter .lightingCenterItem .lightingCenterItemMain {
  height: 8rpx;
  width: 225rpx;
}
.container .lighting .lightingCenter .lightingCenterItem .noVip {
  background: #e5e5e5;
}
.container .lighting .lightingCenter .lightingCenterItem .green {
  background-color: #75d42e;
}
.container .lighting .lightingCenter .green_border {
  border-bottom: 8rpx solid #75d42e !important;
}
.container .lighting .lightingCenter .green_txt {
  color: #75d42e !important;
}
.container .lighting .lightingCenter .blue_border {
  border-bottom: 8rpx solid #3190fe !important;
}
.container .lighting .lightingCenter .blue_txt {
  color: #3190fe !important;
}
.container .lighting .lightingCenter .orange_border {
  border-bottom: 8rpx solid #f5a623 !important;
}
.container .lighting .lightingCenter .orange_txt {
  color: #f5a623 !important;
}
.container .lighting .lignhtingText {
  font-size: 20rpx;
  color: #999999;
  margin-top: 20rpx;
}
.container .lighting .myIdentity {
  padding: 50rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .lighting .myIdentity text {
  font-size: 30rpx;
  font-weight: 400;
  color: #000000;
}
.container .lighting .myIdentity .myIdentityIcon {
  display: flex;
  align-items: center;
}
.container .lighting .myIdentity .myIdentityIcon text {
  font-size: 20rpx;
  margin-right: 20rpx;
  color: #f5a623;
}
.container .lighting .myIdentity .myIdentityIcon .w39h32 {
  width: 39rpx !important;
  height: 32rpx !important;
}
.container .lighting .myIdentity .myIdentityIcon image {
  width: 39rpx;
  height: 32rpx;
  margin-right: 11rpx;
}
.container .lighting .myIdentity .myIdentityIcon image:last-child {
  width: 12rpx;
  height: 21rpx;
  margin-right: 0;
}
.container .list {
  margin-top: 20rpx;
  width: 690rpx;
  margin: 0 auto;
}
.container .list .listItem {
