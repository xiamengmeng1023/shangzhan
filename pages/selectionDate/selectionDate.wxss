.selectionDate_main {
  width: 690rpx;
  margin: 0 auto;
}
.selectionDate_main .selectionDate_main_tit {
  font-size: 36rpx;
  margin-bottom: 30rpx;
  font-weight: 700;
}
.selectionDate_main .selectionDate_main_month_box {
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.selectionDate_main .selectionDate_main_month_box .selectionDate_main_month {
  width: 150rpx;
  text-align: center;
  padding-top: 54rpx;
  color: #999;
  border: 1rpx solid #e5e5e5;
  margin-bottom: 30rpx;
  margin-right: 25rpx;
}
.selectionDate_main .selectionDate_main_month_box .selectionDate_main_month:nth-child(4n) {
  margin-right: 0;
}
.selectionDate_main .selectionDate_main_month_box .selectionDate_main_month .selectionDate_main_month_t {
  font-size: 30rpx;
  margin-bottom: 7rpx;
}
.selectionDate_main .selectionDate_main_month_box .selectionDate_main_month .selectionDate_main_month_b {
  font-size: 20rpx;
  padding-bottom: 19rpx;
}
.activ {
  background-color: #3190fe !important;
  color: #ffffff !important;
}
