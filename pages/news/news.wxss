.m-box {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  /* height: 100%; */
  padding: 40rpx 30rpx;
}

.m-box text:first-child {
  margin-bottom: 120rpx;
}

.t-box {
  width: 100%;
  padding: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
}

.f-title {
  text-align: center;
}

.alert {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  line-height: 60rpx;
  max-height: 360rpx;
  background: rgba(0, 0, 0, 0.6);
}

.comment {
  width: 32rpx;
  height: 32rpx;
}

