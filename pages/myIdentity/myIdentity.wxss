.lm {
  width: 750rpx;
  height: 283rpx;
  position: relative;
}
.lm image {
  width: 750rpx;
  height: 283rpx;
}
.lm .lmText {
  width: 450rpx;
  position: absolute;
  top: 150rpx;
  left: 30rpx;
  display: flex;
  flex-direction: column;
}
.lm .lmText text:nth-child(1) {
  font-size: 28rpx;
  color: #ffea00;
}
.lm .medal {
  width: 750rpx;
  height: 320rpx;
  background: #ffffff;
  box-shadow: 0rpx 10rpx 10rpx 0rpx rgba(229, 229, 229, 0.5);
  border-radius: 30rpx 30rpx 0rpx 0rpx;
  position: absolute;
  top: 241rpx;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.lm .medal .medalItem {
  width: 13.4%;
  height: 192rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.lm .medal .medalItem image {
  height: 150rpx;
  width: 100rpx;
}
.lm .medal .medalItem text {
  font-size: 24rpx;
  font-weight: 400;
  color: #000000;
}
.identity {
  margin-top: 300rpx;
  padding: 30rpx;
}
.identity .identityTop {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.identity .identityTop .vipState_ok {
  font-size: 28rpx;
  font-weight: 400;
  color: #7ed321;
}
.identity .identityTop .vipState_no {
  width: 120rpx;
  height: 50rpx;
  border-radius: 38rpx;
  background-color: #3190FE;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 50rpx;
  box-shadow: #D7E9FF 0rpx 15rpx 10rpx;
}
.identity .identityTop .renew {
  background-color: #7ED321;
}
.identity .identityTop .identityTopLeft {
  display: flex;
  font-weight: 500;
  align-items: flex-end;
}
.identity .identityTop .identityTopLeft .identityTopLeft_tit_one {
  font-size: 30rpx;
  font-weight: 500;
  color: #000000;
}
.identity .identityTop .identityTopLeft .identityTopLeft_tit_two {
  font-size: 24rpx;
  color: #f5a623;
  margin-left: 10rpx;
}
.identityMiddle {
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.identityMiddle .identityMiddleItem {
  display: flex;
  height: 95rpx;
  width: 20%;
  padding-bottom: 50rpx;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.identityMiddle .identityMiddleItem image {
  width: 35%;
  height: 50rpx;
}
.identityMiddle .identityMiddleItem text {
  font-size: 24rpx;
  font-weight: 400;
  color: #999999;
}
.identityBottom {
  text-align: center;
  font-size: 24rpx;
  color: #999999;
  margin-top: 100rpx;
  padding: 10rpx 0;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000000;
  opacity: 0.1;
  overflow: hidden;
  z-index: 998;
  color: #fff;
}
.fxmodal {
  position: fixed;
  bottom: 0;
  width: 690rpx;
  padding: 50rpx 30rpx 76rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  z-index: 999;
}
.fxmodal .fxheader {
  font-size: 30rpx;
  color: #000000;
  display: flex;
  justify-content: flex-start;
}
.fxmodal .fxselect {
  margin-top: 50rpx;
  display: flex;
  justify-content: space-between;
  background: #f5f5f5;
  color: #000000;
}
.fxmodal .fxselect .item {
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
  width: 340rpx;
  height: 125rpx;
  margin-right: 15rpx;
  border-radius: 4rpx;
}
.fxmodal .fxselect .item:last-child {
  margin-right: 0rpx;
}
.fxmodal .fxselect .item text {
  font-size: 24rpx;
  font-weight: 400;
}
.fxmodal .fxselect .item .price {
  display: flex;
  align-items: center;
}
.fxmodal .fxselect .item .price text:nth-child(1) {
  font-size: 30rpx;
  font-weight: 500;
}
.fxmodal .fxselect .item .price text:nth-child(2) {
  margin-left: 15rpx;
  text-decoration: line-through;
  font-size: 24rpx;
  font-weight: 500;
}
.fxmodal .fxselect .select {
  background: #3190fe !important;
  color: #ffffff !important;
}
.listPrice {
  margin-top: 24rpx;
}
.listPrice .listItem {
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx #e5e5e5 solid;
}
.listPrice .listItem .priceName {
  font-size: 30rpx;
  color: #000000;
}
.listPrice .listItem .oldPrice {
  font-size: 36rpx;
  color: #999;
  text-decoration: line-through;
}
.listPrice .listItem .defaultPrice {
  font-size: 36rpx;
  color: #3190fe;
}
.listPrice .listItem .normalPrice {
  font-size: 36rpx;
  color: #000000;
}
.erweima {
  margin: 0 auto;
}
.erweima image {
  width: 500rpx;
  height: 500rpx;
}
.true {
  width: 690rpx;
  height: 88rpx;
  margin-top: 76rpx;
  background: #3190fe;
  border-radius: 5rpx;
  text-align: center;
  line-height: 88rpx;
  color: #fff;
}
.blue {
  color: #3190FE !important;
}
