.f-box{
  min-height: 90%;
  border-radius: 16rpx;
  overflow: hidden;
  box-shadow: 0 0 20rpx 10rpx #e7e7e8;
}
.bg-box{
  position: relative;
}
.bg-box image{
  width: 100%;
  height: 318rpx;
}
.m-box{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
}
.m-box text:first-child{
  margin-bottom: 170rpx;
}
.t-box{
  width: 100%;
  padding: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
}
.f-title{
  text-align: center;
}

.log-img{
  width: 42rpx;
  height: 42rpx;
}
