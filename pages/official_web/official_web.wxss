}

.list_txt3>text {
   font-size: 24rpx;
   color: #999;
   position: relative;
   height: 33rpx;
   margin-top: 12rpx;
}

.list_txt3>text::after {
   content: '';
   width: 100%;
   height: 1px;
   background: #999;
   position: absolute;
   left: 0;
   top: 50%;
}

