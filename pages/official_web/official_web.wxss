   width: 100%;
   height: 240rpx;
}

.brief_2 {
   margin-bottom: 50rpx;
}

.brief_2 image {
   width: 100%;
   height: 240rpx;
   margin-bottom: 30rpx;
}

.brief_2 text {
   font-size: 30rpx;
   line-height: 50rpx;
   padding: 30rpx;
   color: #666;
   display: block;
   text-align: justify;
}

.succ_img {
   width: 225rpx;
   height: 225rpx;
   position: relative;
   margin: 0 8rpx 8rpx 0;
}

.succ_img:nth-child(3n) {
   margin-right: 0;
}

.succ_img image {
   width: 100%;
   height: 100%;
}

.succ_img view {
   width: 100%;
   height: 100%;
   background: rgba(0, 0, 0, .3);
   position: absolute;
   left: 0;
   top: 0;
}

.succ_img text {
   font-size: 33rpx;
   color: #fff;
   width: 100%;
   height: auto;
   text-align: center;
   position: absolute;
   left: 0;
   top: 50%;
   transform: translate(0, -50%);
}

.succ_img_2 {
   width: 340rpx;
   margin: 0 10rpx 50rpx 0;
}

.succ_img_2:nth-child(2n) {
   margin: 0 0 50rpx 0;
}

.succ_img_2 image {
   width: 100%;
   height: 340rpx !important;
   margin-bottom: 30rpx !important;
}

.succ_img_2 text {
   display: block;
   color: #000;
   font-size: 26rpx;
   overflow: hidden;
   white-space: nowrap;
   text-overflow: ellipsis;
}

.succ_img_2.succ_img_3 image {
   height: 460rpx !important;
}

