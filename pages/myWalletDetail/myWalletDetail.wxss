.wallet_d {
   padding: 0 30rpx;
   height: 100%;
   position: relative;
}

.minimum {
   padding: 35rpx 0;
   font-size: 24rpx;
   color: #999;
   border-bottom: 1px solid #E5E5E5;
}

.cash_my {
   padding: 30rpx 0;
   border-bottom: 1px solid #E5E5E5;
}

.cash_my>text {
   font-size: 24rpx;
   color: #000;
   margin-bottom: 20rpx;
   display: block;
}

.input text {
   font-size: 36rpx;
   color: #000;
   display: inline-block;
   vertical-align: top;
   margin: 30rpx 0 0 48rpx;
}

.input input {
   width: 80%;
   height: 140rpx;
   font-size: 100rpx;
   color: #000;
   padding: 0 20rpx;
   box-sizing: border-box;
   display: inline-block;
}

.all_cash {
   margin-top: 50rpx;
   display: flex;
   align-items: center;
   font-size: 24rpx;
}

.all_cash text:nth-child(1) {
   color: #999;
}

.all_cash text:nth-child(2) {
   color: #0090FF;
   margin-left: 10rpx;
}

.submit {
   width: 690rpx;
   height: 88rpx;
   line-height: 88rpx;
   text-align: center;
   font-size: 30rpx;
   color: #fff;
   background: #3190FE;
   margin: 120rpx auto 0;
   margin-bottom: 455rpx
}

.hint {
   width: 690rpx;
   height: 80rpx;
   line-height: 40rpx;
   font-size: 24rpx;
   color: #999;
   text-align: center;
   position: absolute;
   left: 30rpx;
   bottom: 50rpx;
}