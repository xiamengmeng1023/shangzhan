.header {
  width: 100%;
  position: fixed;
  background-color: #fff;
  top: 0rpx;
  left: 50%;
  transform: translate(-50%);
}
.header .header_banner {
  color: #999999;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
}
.header .header_banner .banner_item {
  font-size: 30rpx;
  border-bottom: 6rpx solid #fff;
  padding-bottom: 30rpx;
  padding-top: 30rpx;
}
.header .header_banner .banner_item:first-child {
  margin-right: 130rpx;
}
.header .header_banner .active {
  color: #000 !important;
  border-bottom: 6rpx solid #3190FE;
}
.ticket {
  margin-top: 110rpx;
  padding: 30rpx 30rpx;
}
.usefulTicketItem {
  height: 200rpx;
  display: flex;
  flex-direction: column;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
  box-shadow: 0px 10px 10px 0px rgba(229, 229, 229, 0.5);
  border: 1px solid #f5f5f5;
}
.usefulTicketItem .blue {
  background-color: #F4FAFF !important;
}
.used {
  background: #f4faff;
}
.used .ticketItemTitleuser {
  color: #999999;
  background: #f4faff;
  border: 2px solid #e5e5ee;
  box-shadow: none;
}
.expired .ticketItemTitleuser {
  background: #fff;
  color: #999999;
  box-shadow: none;
}
.ticketItemTitle {
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ticketItemTitleContent {
  display: flex;
  align-items: center;
}
.icon {
  width: 70rpx;
  height: 70rpx;
}
.ticketItemDetail {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
}
.ticketItemDetail text:nth-child(1) {
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #000000;
}
.ticketItemDetail text:nth-child(2) {
  margin-top: 10rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #000000;
}
.ticketItemTitleuser {
  width: 130rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #ffffff;
  background: #328ee9;
  box-shadow: 0px 10px 10px 0px #d3e7ff;
  border-radius: 40px;
}
.ticketItemContent {
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 1px solid #f5f5f5;
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #999999;
}
.noneBox {
  display: flex;
  background-color: #fff;
  flex-direction: column;
  align-items: center;
  height: 100%;
}
.noneBox .noneTxt {
  font-size: 40rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%);
  text-align: center;
  font-weight: 700;
  color: #999;
}
